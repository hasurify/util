{
  "name": "@hasurify/util",
  "version": "1.0.0",
  "description": "Hasurify Util Library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsc -w",
    "tsc": "tsc --build",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}' --quiet --fix",
    "commit": "cz",
    "semantic-release": "semantic-release",
    "test": "jest",
    "coverage": "nyc --reporter lcov yarn test",
    "coverage:ci": "JEST_JUNIT_OUTPUT_DIR='coverage' npx jest --ci --coverage --maxWorkers 4 --testLocationInResults",
    "coverage:ci:summary": "yarn coverage:ci | tee ./coverage.txt"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hasurify/util.git"
  },
  "author": "KenStack Team",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cli-color": "^2.0.3",
    "countries-and-timezones": "^3.4.1",
    "dayjs": "^1.11.7",
    "dayjs-precise-range": "^1.0.1",
    "geo-tz": "^7.0.7",
    "js-base64": "^3.7.5",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "uuid": "^9.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cli-color": "^2.0.2",
    "@types/countries-and-timezones": "^3.2.3",
    "@types/jest": "^29.5.4",
    "@types/js-base64": "^3.3.1",
    "@types/lodash": "^4.14.194",
    "@types/md5": "^2.3.2",
    "@types/mime-types": "^2.1.0",
    "@types/node-fetch": "^2.5.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "lint-staged": "^11.0.1",
    "module-alias": "^2.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^21.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/hasurify/graphql/issues"
  },
  "homepage": "https://github.com/hasurify/graphql#readme"
}
